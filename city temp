        "\n",
        "stats_summary = df.groupby(\"City\")[\"Temperature\"].agg(\n",
        "    Mean=\"mean\",\n",
        "    Median=\"median\",\n",
        "    Mode=lambda x: stats.mode(x, keepdims=False).mode\n",
        ").reset_index()\n",
        "\n",
        "print(\" Comparative Temperature Statistics:\\n\")\n",
        "print(stats_summary)"
